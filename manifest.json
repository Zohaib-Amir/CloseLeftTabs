{
  "manifest_version": 3,

  "name": "Close Left tabs",
  "version": "0.4",
  "description": "Close Left Tabs on one click",

  "permissions": ["contextMenus"],

  "action": {
    "default_icon": "icon.png",
    "default_title": "Close Left Tabs"
  },

  "background": {
